class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int curSum = 0;
        unordered_map<int, int> map;
        
        map[0] = 1;
        int result = 0;

        for(int num: nums){
            curSum += num;
            result += map[curSum-k];
            map[curSum]++;
        }
        return result;
    }
};
